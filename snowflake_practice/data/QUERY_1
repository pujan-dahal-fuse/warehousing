CREATE WAREHOUSE MY_WAREHOUSE;

CREATE DATABASE MY_DATABASE;

USE DATABASE MY_DATABASE;
CREATE SCHEMA FIRST_SCHEMA;

CREATE OR REPLACE TABLE 
"MY_DATABASE"."FIRST_SCHEMA"."FEATURES"
(
"STORE" INTEGER,
  "DATE" DATE,
  "TEMPERATURE" FLOAT,
  "FUEL_PRICE" FLOAT,
  "MARKDOWN1" STRING,
  "MARKDOWN2" STRING,
  "MARKDOWN3" STRING,
  "MARKDOWN4" STRING,
  "MARKDOWN5" STRING,
  "CPI" STRING,
  "UNEMPLOYMENT" STRING,
  "ISHOLIDAY" STRING
);


SELECT * FROM FEATURES;